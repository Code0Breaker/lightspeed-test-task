    "@vitejs/plugin-vue": "^5.1.4",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "3.3.3",
    "sass": "^1.79.5",
    "sass-loader": "^16.0.2",
    "typescript": "^5.5.3",
    "vite": "^5.4.8",
    "vue-tsc": "^2.1.6"
  }
